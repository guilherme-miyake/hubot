{
  "name": "hubot",
  "version": "0.0.0-development",
  "author": "hubot",
  "keywords": [
    "github",
    "hubot",
    "campfire",
    "bot"
  ],
  "description": "A simple helpful robot for your Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hubotio/hubot.git"
  },
  "dependencies": {
    "async": ">=0.1.0 <1.0.0",
    "chalk": "^1.0.0",
    "cline": "^0.8.2",
    "coffeescript": "1.6.3",
    "connect-multiparty": "^2.1.1",
    "express": "^4.16.3",
    "log": "1.4.0",
    "optparse": "1.0.4",
    "scoped-http-client": "0.11.0",
    "typescript": "^4.1.3"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@octokit/core": "^3.2.4",
    "@types/async": "^3.2.5",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.20",
    "@types/scoped-http-client": "^0.11.0",
    "chai": "~2.1.0",
    "coveralls": "^3.0.2",
    "is-circular": "^1.0.2",
    "mocha": "^8.2.1",
    "mockery": "^1.4.0",
    "nyc": "^15.1.0",
    "semantic-release": "^17.3.1",
    "sinon": "~1.17.0",
    "sinon-chai": "^2.8.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.14"
  },
  "engines": {
    "node": "> 4.0.0",
    "npm": "> 2.0.0"
  },
  "main": "./index",
  "bin": {
    "hubot": "./bin/hubot"
  },
  "scripts": {
    "build": "tsc",
    "start": "bin/hubot",
    "test": "tsc && nyc --reporter=html --reporter=text --extends=@istanbuljs/nyc-config-typescript mocha --require ts-node/register --require source-map-support/register test/*.ts",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test:smoke": "node src/**/*.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  }
}
